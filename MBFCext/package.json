{
  "dependencies": {
    "chrome-options": "^1.0.0",
    "chrome-promise": "^3.0.5",
    "debug": "^4.3.0",
    "jquery": "^3.5.1",
    "lodash": "^4.17.20"
  },
  "description": "Facebook News Biases",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@types/chrome": "0.0.124",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/debug": "^4.1.5",
    "@types/file-loader": "^4.2.0",
    "@types/fs-extra": "^9.0.2",
    "@types/google.analytics": "0.0.40",
    "@types/jquery": "^3.5.3",
    "@types/lodash": "^4.14.162",
    "@types/minimist": "^1.2.0",
    "@types/moment": "^2.13.0",
    "@types/prettier": "^2.1.2",
    "@types/rimraf": "^3.0.0",
    "@types/typescript": "^2.0.0",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "crx": "^5.0.1",
    "css-loader": "^4.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "file-loader": "^6.1.1",
    "fontawesome-svg-sprite-generator": "^1.0.2",
    "fs-extra": "^9.0.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "markdown-to-html": "0.0.13",
    "minimist": "^1.2.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.4",
    "typescript": "~4.0.3",
    "webextension-polyfill": "^0.6.0",
    "webpack": "4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-extension-reloader": "^1.1.4",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "name": "mbfc-chrome-extension",
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "clean": "rimraf build/ dev/ *.zip *.crx",
    "compress-keygen": "crx keygen",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "posttest": "npm run format",
    "pretest": "npm run lint",
    "release": "npm run release-notes && npm run update-versions && npm run clean && npm run build && node scripts/compress",
    "release-notes": "markdown release-notes/ReleaseNotes.md > release-notes/ReleaseNotes.html && echo -n \"export const ReleaseNotes = \\`\" > src/ReleaseNotes.ts && cat release-notes/ReleaseNotes.html >> src/ReleaseNotes.ts && echo \"\\`;\" >> src/ReleaseNotes.ts",
    "start": "NODE_ENV=development webpack --watch",
    "update-versions": "export VERSION=$(jq -r .version package.json) && jq \".version=\\\"$VERSION\\\"\" src/manifest.json | jq \".version_name=\\\"Release $VERSION\\\"\" > manifest.new && mv manifest.new src/manifest.json && git add src/manifest.json release-notes/* src/ReleaseNotes.ts && git commit -m\"Release $VERSION\"; exit 0"
  },
  "version": "2.1.2"
}
